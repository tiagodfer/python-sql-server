The tables are structured as follows:
    - cpf.db:
        - TABLE cpf:
            - cpf VARCHAR;
            - nome VARCHAR;
            - sexo CHAR;
            - nasc DATE.
    - cnpj.db:
        - TABLE socios:
            - radical TEXT;
            - tipo TEXT;
            - nome TEXT;
            - cpf_cnpj TEXT;
            - qualificacao TEXT;
            - data TEXT;
            - pais TEXT;
            - representante_legal TEXT;
            - nome_representante TEXT;
            - qualificacao_representante TEXT;
            - faixa_etaria TEXT.
        - TABLE empresas;
            - radical TEXT;
            - razao TEXT;
            - natureza TEXT;
            - qualificacao TEXT;
            - capital TEXT;
            - porte TEXT;
            - responsavel TEXT.
        - TABLE estabelecimentos;
            - radical TEXT;
            - ordem TEXT;
            - dv TEXT;
            - matriz_filial TEXT;
            - nome_fantasia TEXT;
            - situacao TEXT;
            - data_situacao TEXT;
            - motivo_situacao TEXT;
            - cidade_exterior TEXT;
            - pais TEXT;
            - data_inicio_atividade TEXT;
            - cnae_principal TEXT;
            - cnae_secundario TEXT;
            - tipo_logradouro TEXT;
            - logradouro TEXT;
            - numero TEXT;
            - complemento TEXT;
            - bairro TEXT;
            - CEP TEXT;
            - UF TEXT;
            - municipio TEXT;
            - ddd1 TEXT;
            - fone1 TEXT;
            - ddd2 TEXT;
            - fone2 TEXT;
            - dddfax TEXT;
            - fax TEXT;
            - email TEXT;
            - situacao_especial TEXT;
            - data_situacao_especial TEXT.
        - TABLE cnaes;
            - id TEXT;
            - desc TEXT.
        - TABLE municipios;
            - id TEXT;
            - desc TEXT.
        - TABLE qualificacoes;
            - id TEXT;
            - desc TEXT.
        - TABLE paises;
            - id TEXT;
            - desc TEXT.
        - TABLE motivos;
            - id TEXT;
            - desc TEXT.
        - TABLE naturezas;
            - id TEXT;
            - desc TEXT.
